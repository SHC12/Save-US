<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="superhumancrew.com.saveus.MainActivity">
<TextView
    android:id="@+id/txtJudul"
    android:textSize="30dp"
    android:text="Nama"
    android:layout_width="match_parent"
    android:layout_height="wrap_content" />
<Button
    android:id="@+id/btnLapor"
    android:text="Lapor"
    android:layout_below="@id/txtJudul"
    android:layout_width="368dp"
    android:layout_height="wrap_content"
    />
    <Button
        android:id="@+id/btnPihakBerwajib"
        android:text="Pihak Berwajib"
        android:layout_width="368dp"
        android:layout_below="@id/btnLapor"
        android:layout_height="wrap_content"
        />
    <Button
        android:id="@+id/btnKeluar"
        android:text="Log out"
        android:layout_width="368dp"
        android:layout_marginTop="50dp"
        android:layout_below="@id/btnPihakBerwajib"
        android:layout_height="wrap_content"
        />


</RelativeLayout>
